    </resultMap>
    <sql id="Base_Column_List">
        id, user_id, user_name, phone, lat, lng, location, address, is_default_address, create_time,
    update_time
    </sql>
    <select id="getInsuranceDataList" resultMap="BaseResultMap" parameterType="map">
        select
        <include refid="Base_Column_List"/>
        from insurance_order io
        where 1 = 1
        <if test="userName != null and userName != ''">
            and io.name like concat('%', #{userName}, '')
        </if>
        <if test="insuranceOrderNo != null and insuranceOrderNo != ''">
            and io.insuranceOrderNo like concat('%', #{insuranceOrderNo}, '%')
        </if>
        <if test="start != null">
            and io.applyTime &gt; #{start}
        </if>
        <if test="end != null">
            and io.applyTime &lt; #{end}
        </if>
        order by io.applyTime desc
    </select>
